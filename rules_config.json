{
    "collections": [
        {
            "description": "Rapid API payments",
            "predicate": "All",
            "rules": [
                {
                    "field_name": "From",
                    "predicate": "contains",
                    "value": "rapidapi"
                },
                {
                    "field_name": "Subject",
                    "predicate": "contains",
                    "value": "payment"
                },
                {
                    "field_name": "Date Received",
                    "predicate": "is less than",
                    "value": "1 years"
                }
            ],
            "actions": [
                {
                    "type": "Mark Message As",
                    "value": "Read"
                },
                {
                    "type": "Move Message To",
                    "value": "Inbox"
                }
            ]
        },
        {
            "description": "Rapid API subscription",
            "predicate": "All",
            "rules": [
                {
                    "field_name": "From",
                    "predicate": "contains",
                    "value": "rapidapi"
                },
                {
                    "field_name": "Message",
                    "predicate": "contains",
                    "value": "ULTRA plan"
                },
                {
                    "field_name": "Date Received",
                    "predicate": "is less than",
                    "value": "4 months"
                }
            ],
            "actions": [
                {
                    "type": "Move Message To",
                    "value": "Inbox"
                }
            ]
        }
    ]
}